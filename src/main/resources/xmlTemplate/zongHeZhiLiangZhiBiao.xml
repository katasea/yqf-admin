<?xml version="1.0" encoding="UTF-8"?>
<excel>
   <entity name="质量综合指标信息" code="pm_norm" primarykey="uid">
       <column name="uid" code="t1.uid"  type="String" seq="0">
		    <rules> 
				<rule name="nullable" message="id不允许为空！"></rule>
				<rule name="length" length ="1,20" message="指标名称长度必须介于1到20之间！"></rule>
