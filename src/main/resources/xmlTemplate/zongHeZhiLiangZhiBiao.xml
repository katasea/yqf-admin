				<rule name="nullable" message="指标名称不允许为空！"></rule>
				<rule name="length" length ="1,20" message="指标名称长度必须介于1到20之间！"></rule>
				<rule name="unique" tableName="pm_norm" colName="text" message="系统中已存在相同的指标名称！"></rule>
			</rules>
		</column>
		<column name="父id" code="t1.parentid"  type="String" seq="2">
		</column>
   </entity>
